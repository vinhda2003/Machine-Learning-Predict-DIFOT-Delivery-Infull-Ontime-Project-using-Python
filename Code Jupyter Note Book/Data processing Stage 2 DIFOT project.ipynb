    "        lit('-'),\n",
    "        col(\"isfreegood_order_no\"),\n",
    "        lit('-'),\n",
    "        col(\"isfreegood_customer_code\")\n",
    "    )\n",
    ")\n",
    "\n",
    "# Join isfreegood with factdata\n",
    "filtered_outliers_df = filtered_outliers_df.join(isfreegood_renamed, \"key\", \"left\")\n",
